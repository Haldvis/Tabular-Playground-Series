%matplotlib inline 


data = pd.read_csv('../input/tabular-playground-series-jun-2022/data.csv', index_col='row_id')
submission = pd.read_csv('../input/tabular-playground-series-jun-2022/sample_submission.csv')#, index_col='row-col')

